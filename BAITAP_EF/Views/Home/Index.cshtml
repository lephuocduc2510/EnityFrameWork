@model List<BAITAP_EF.Models.Students>

    @{
    ViewBag.Title = "Index";
}

<h2 style="text-align:center">CODE FIRST</h2>
<table class="table">
    <thead>
        <tr>
            <th>STT</th>
            <th>Họ và Tên</th>
            <th>Địa chỉ</th>
            <th>Số điện thoại</th>
            <th>Tuổi</th>

        </tr>
        @foreach (var item in ViewBag.listStudents)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Address</td>
                <td>@item.Phone</td>
                <td>@item.Age</td>
                
            </tr>
        }
    </thead>
</table>
<style>
    table {
        width: 100%;
        border-collapse: collapse;
    }

    table, th, td {
        border: 1px solid black;
        
    }
    </style>